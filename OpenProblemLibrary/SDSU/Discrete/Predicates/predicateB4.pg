## DBsubject('Discrete Mathematics')
## DBchapter('Logic')
## DBsection('First Order Logic')
## KEYWORDS('predicate')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Thomas Schmidt','Michael E. O'Sullivan')
## Institution('SDSU')


############################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

TEXT(beginproblem());

############################
# Setup

$showPartialCorrectAnswers = 0;

$num = 12;
@factors = (1,2,3,4,6,12);
$tmp = join(",",@factors);
$answer = Compute($tmp);

############################
# Main Text

Context()->texStrings;
BEGIN_TEXT
Let \(F(z)\) be the predicate \(z\text{ is a factor of } $num\). $PAR
Determine the truth set for \(F(z)\). $BR
\(\lbrace\)\{ans_rule(15)\}\(\rbrace\)
END_TEXT
Context()->normalStrings;

############################
# Answer Evaluation

ANS($answer->cmp());

############################
# Solution

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR Solution: $PAR
\(F(z)\) is the predicate \(z\text{ is a factor of } $num\). $PAR
The truth set for \(F(z)\) would then be $BR
\(\lbrace 1,2,3,4,6,12\rbrace\) $PAR
END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
