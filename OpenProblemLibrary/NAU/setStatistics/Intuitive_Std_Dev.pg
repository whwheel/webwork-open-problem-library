DOCUMENT();     
loadMacros("PGstandard.pl",
           "extraAnswerEvaluators.pl",
           "PGnauStats.pl"
);

TEXT(&beginproblem) ;
$showPartialCorrectAnswers = 1;

@num1=(1,3,4,6,7,10);
@num2=(1,1,5,7,12,15);
@num3=(1,2,3,3,3,5);
@nnum1=();
@nnum2=();
@nnum3=();

$a=random(40,90,1);
$b=random(5,20,1);
$c=random(3,7,1);
for($i=0; $i<6; $i++){
    $nnum1[$i]=$num1[$i]+$a;
    $nnum2[$i]=$num2[$i]+$b;
    $nnum3[$i]=$num3[$i]+$c;
}
$nums1=join(", ",@nnum1);
$nums2=join(", ",@nnum2);
$nums3=join(", ",@nnum3);

@D=('larger', 'smaller');
@ans=('B','C');
$f=random(0,1,1);

BEGIN_TEXT
$BR
Data set A: \[ $nums1 \] 
$BR
Data set B: \[ $nums2 \] 
$BR
Data set C: \[ $nums3 \] 
$BR
Above are three different data sets. Without calculating anything, decide which set A, B, or C has the $D[$f] 
standard deviation. Just enter the letter of the data set as your answer.
$BR
$BR 
\{ans_rule(15)\} 
$BR
END_TEXT

ANS(str_cmp($ans[$f]));

ENDDOCUMENT();